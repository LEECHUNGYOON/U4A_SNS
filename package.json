{
  "name": "com.u4a_sns.app",
  "displayName": "u4a_sns",
  "description": "A U4A IDE Platform R&D Team application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "U4A R&D Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-electron": "^3.0.0",
    "cordova-plugin-whitelist": "^1.3.5"
  },
  "cordova": {
    "platforms": [
      "electron"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LEECHUNGYOON/U4A_SNS.git"
  },
  "build": {
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "win": {},
    "publish": {
      "provider": "github"
    },
    "files": [
      "**/*",
      "./node_modules/**/*",
      "./package.json"
    ],
    "extraResources": [
      "./node_modules/**/*",
      "./node_modules/@electron/remote/**",
      {
        "from": "node_modules/regedit/vbs",
        "to": "regedit/vbs",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@octokit/core": "^4.1.0",
    "axios": "^1.1.3",
    "electron-updater": "^5.3.0",
    "form-data": "^4.0.0",
    "http": "^0.0.1-security",
    "ip": "^2.0.0",
    "mime-types": "^2.1.35",
    "mongodb": "^4.11.0",
    "multiparty": "^4.2.3",
    "node-telegram-bot-api": "^0.59.0",
    "open": "^8.4.0",
    "querystring": "^0.2.1",
    "random-key": "^0.3.2",
    "youtube-api": "^3.0.1"
  }
}
